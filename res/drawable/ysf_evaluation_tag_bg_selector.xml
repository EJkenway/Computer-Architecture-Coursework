<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false">
        <shape>
            <solid android:color="@color/ysf_white" />
            <stroke android:width="0.5dip" android:color="#ffe1e3e6" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/ysf_white" />
            <stroke android:width="0.5dip" android:color="@color/ysf_blue_337EFF" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
</selector>