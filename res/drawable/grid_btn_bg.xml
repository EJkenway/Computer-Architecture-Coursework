<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/AU_COLOR_DIALOG_LIST_PRESS" />
            <corners android:radius="@dimen/AU_CORNER2" />
            <stroke android:width="@dimen/AU_DIVIDER_SPACE1" android:color="@color/AU_COLOR_ASS_CONTENT" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/AU_CORNER2" />
            <stroke android:width="@dimen/AU_DIVIDER_SPACE1" android:color="@color/AU_COLOR_ASS_CONTENT" />
        </shape>
    </item>
</selector>