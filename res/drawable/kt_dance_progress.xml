<?xml version="1.0" encoding="utf-8"?>
<layer-list android:layout_width="fill_parent" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <stroke android:width="2.0dip" android:color="@color/white" />
            <corners android:radius="8.0dip" />
            <solid android:color="@color/kt_color_111113_47" />
            <padding android:left="@dimen/dimen_4dp" android:top="@dimen/dimen_4dp" android:right="@dimen/dimen_4dp" android:bottom="@dimen/dimen_4dp" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="8.0dip" />
                <gradient android:startColor="@color/kt_color_DFEEFF" android:endColor="@color/kt_color_FED5FF" android:angle="0.0" android:centerColor="@color/kt_color_F9F4FF" />
            </shape>
        </scale>
    </item>
</layer-list>