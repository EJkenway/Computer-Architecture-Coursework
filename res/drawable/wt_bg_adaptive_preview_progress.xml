<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/white_10" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff0f5e37" android:endColor="#ff4ee3d4" android:angle="0.0" android:type="linear" android:centerColor="#ff24c789" />
                <corners android:radius="1.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>