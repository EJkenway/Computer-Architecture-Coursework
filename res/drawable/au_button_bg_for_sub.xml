<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#66ffffff" />
            <stroke android:width="@dimen/au_button_outline_width" android:color="#66e5e5e5" />
            <corners android:radius="@dimen/AU_CORNER1" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="#ffe5e5e5" />
            <stroke android:width="@dimen/au_button_outline_width" android:color="#ffcecece" />
            <corners android:radius="@dimen/AU_CORNER1" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#ffffffff" />
            <stroke android:width="@dimen/au_button_outline_width" android:color="#ffe5e5e5" />
            <corners android:radius="@dimen/AU_CORNER1" />
        </shape>
    </item>
</selector>